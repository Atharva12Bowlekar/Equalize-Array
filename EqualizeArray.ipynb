    "def equalizeArray(arr):\n",
    "    setter=set(arr)\n",
    "    c=[0]*max(arr)\n",
    "    m=0\n",
    "    for i in setter:\n",
    "        k=0\n",
    "        k=arr.count(i)\n",
    "        c.insert(i,k)\n",
    "    m=(max(c))\n",
    "    c.remove(m)\n",
    "    return sum(c)"
   ]
